<Window x:Class="main.uklady"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:main"
        mc:Ignorable="d"
        Title="uklady" Height="450" Width="800">
    <StackPanel>
        <Button Content="1"></Button>
        <Button Content="2"></Button>
        <DockPanel>
            <Button DockPanel.Dock="Left" Content="3"></Button>
            <Button DockPanel.Dock="Right" Content="4"></Button>
            <Button DockPanel.Dock="Bottom" Content="5"></Button>
            <Button DockPanel.Dock="Top" Content="6"></Button>
        </DockPanel>
    </StackPanel>
</Window>
